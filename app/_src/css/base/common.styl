



/* Other Common Styles
   ========================================================================= */
/**
 * Floats
 */
.fl {
    float left;
}
.fr {
    float right;
}

/**
 * Clearfix
 */
.clearfix:after{
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0;
}
    
.clearfix{
    *zoom: 1;
}